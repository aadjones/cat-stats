{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/legend/(.*)",
      "destination": "/index.html"
    },
    {
      "source": "/showdown/(.*)",
      "destination": "/index.html"
    },
    {
      "source": "/hall-of-fame",
      "destination": "/index.html"
    }
  ]
}
